{
    "name": "sandritsch91/yii2-honey-pot",
    "description": "AntiSpam for Yii2",
    "type": "yii2-extension",
    "keywords": [
        "AntiSpam",
        "HoneyPot",
        "Hash",
        "Spam",
        "yii2"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Sandritsch91",
        "email": "sandritsch91@gmail.com"
    }],
    "support": {
        "source": "https://github.com/Sandritsch91/yii2-honey-pot",
        "issues": "https://github.com/Sandritsch91/yii2-honey-pot/issues"
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=8.1",
        "yiisoft/yii2": "^2.0.50"
    },
    "require-dev": {
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-bootstrap4": "*",
        "yiisoft/yii2-bootstrap5": "*",
        "kartik-v/yii2-password": "*",
        "raiffeisen/bootstrap": "*",
        "sandritsch91/yii2-widget-flatpickr": "*"
    },
    "autoload": {
        "psr-4": {
            "sandritsch91\\yii2\\honeypot\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
